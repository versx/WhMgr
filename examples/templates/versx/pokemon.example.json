{
    "style": "klokantech-basic",
    "latitude": <%= lat %>,
    "longitude": <%= lon %>,
    "zoom": 15,
    "width": 300,
    "height": 175,
    "scale": 1,
    "markers": [
        <% if (pokestops != null) {
            pokestops.forEach(function(pokestop) { %>
            {
                "url": "<%= pokestop.marker %>",
                "latitude": <%= pokestop.lat %>,
                "longitude": <%= pokestop.lon %>,
                "width": 32,
                "height": 32
            },
            <% });
        } %>
        <% if (gyms != null) {
            gyms.forEach(function(gym) { %>
            {
                "url": "<%= gym.marker %>",
                "latitude": <%= gym.lat %>,
                "longitude": <%= gym.lon %>,
                "width": 32,
                "height": 32
            },
            <% });
        } %>
    {
        "url": "https://raw.githubusercontent.com/nileplumb/PkmnHomeIcons/ICONS/ICONS/misc/grass.png",
        "latitude": <%= lat %>,
        "longitude": <%= lon %>,
        "x_offset": 0,
        "y_offset": 0,
        "width": 48,
        "height": 48
    },{
        "url": "<%= url2 %>",
        "latitude": <%= lat %>,
        "longitude": <%= lon %>,
        "x_offset": 0,
        "y_offset": 0,
        "width": 32,
        "height": 32
    }]
}